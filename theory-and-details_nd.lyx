\begin_layout Subsection
Planar channel (2D) simulations
\end_layout

\begin_layout Standard
The bottom branch contains water flowing at 
\begin_inset Formula $\unit[6.0042]{\si{\micro\litre\per\hour}=\unit[\num{1.6678e6}]{\si{\micro\meter\cubed\per\second}}}$
\end_inset

.
 Water is considered Newtonian, with 
\begin_inset Formula $n=1$
\end_inset

 and 
\begin_inset Formula $K=\mu$
\end_inset

.
\begin_inset Formula 
\begin{align*}
u_{\mathrm{sim}}(y) & =u_{m,\mathrm{sim}}\left\{ 1-\left(\frac{y}{1/2}\right)^{1+1/1}\right\} =u_{m,\mathrm{sim}}\left\{ 1-\left(\frac{y}{0.5}\right)^{2}\right\} \\
\\
u_{m,\mathrm{sim}} & =Q_{\mathrm{exp}}\left(\frac{\mathcal{L}^{3}}{\mathcal{T}}\right)\left(\frac{1}{A_{\mathrm{sim}}}\right)\left(\frac{2+1/n}{1+1/n}\right)\\
 & =\left(\unit[\num{1.6678e6}]{\si{\micro\meter\cubed\per\second}}\right)\left(\frac{\left(\unit[1]{\mathbb{L}/}\unit[48]{\si{\micro\metre}}\right)^{3}}{\unit[1]{\unit[\mathbb{T}]{\si{\per\second}}}}\right)\left(\frac{1}{\left(\unit[1]{\mathbb{L}}\right)^{2}}\right)\left(\frac{3}{2}\right)\\
 & =\unit[\num{22.620}]{\unit[\mathbb{L}]{\mathbb{T}^{-1}}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
Based on the reference viscosity for pure water at atmospheric pressure
 and 
\begin_inset Formula $\unit[25]{\si{\celsius}}$
\end_inset

, the simulation viscosity for water is
\begin_inset Formula 
\begin{align*}
\mu_{\mathrm{sim}} & =\left(\unit[\num{8.9e-4}]{\si{\kilogram\per\meter\per\second}}\right)\left(\frac{\unit[\num{9.069454e9}]{\unit[\mathbb{M}]{\si{\per\kilogram}}}}{\left(\unit[1]{\mathbb{L}/}\unit[48]{\si{\micro\metre}}\right)\left(\unit[1]{\unit[\mathbb{T}]{\si{\per\second}}}\right)}\right)\\
 & =\unit[\num{3.8745e2}]{\mathbb{M}\mathbb{L}^{-1}\mathbb{T}^{-1}}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
The top branch contains Polyox WSR-301 flowing at 
\begin_inset Formula $\unit[6.27]{\si{\micro\litre\per\hour}}=\unit[\num{1.742e6}]{\si{\micro\meter\cubed\per\second}}$
\end_inset

.
 The power-law model parameters for the 0.3% Polyox solution were estimated
 as 
\begin_inset Formula $K_{\mathrm{exp}}=\unit[0.02519]{kg\, m^{-1}\, s^{n-2}}$
\end_inset

 and 
\begin_inset Formula $n=0.7859$
\end_inset

.
\begin_inset Formula 
\begin{align*}
u_{\mathrm{sim}}(y) & =u_{m,\mathrm{sim}}\left\{ 1-\left(\frac{y}{\tfrac{58}{48}/2}\right)^{1+1/0.7859}\right\} =u_{m,\mathrm{sim}}\left\{ 1-\left(\frac{y}{0.6042}\right)^{2.272}\right\} \\
\\
u_{m,\mathrm{sim}} & =Q_{\mathrm{exp}}\left(\frac{\mathcal{L}^{3}}{\mathcal{T}}\right)\left(\frac{1}{A_{\mathrm{sim}}}\right)\left(\frac{2+1/n}{1+1/n}\right)\\
 & =\left(\unit[\num{1.742e6}]{\si{\micro\meter\cubed\per\second}}\right)\left(\frac{\left(\unit[1]{\mathbb{L}/}\unit[48]{\si{\micro\metre}}\right)^{3}}{\unit[1]{\unit[\mathbb{T}]{\si{\per\second}}}}\right)\left(\frac{1}{\left(\unit[\nicefrac{58}{48}]{\mathbb{L}}\right)\left(\unit[1]{\mathbb{L}}\right)}\right)\left(\frac{2+1/0.7859}{1+1/0.7859}\right)\\
 & =\unit[\num{18.772}]{\unit[\mathbb{L}]{\mathbb{T}^{-1}}}\\
\\
K_{\mathrm{sim}} & =\left(\unit[\num{2.519e-2}]{\si{\kilogram\per\meter\per\second}}\right)\left(\frac{\unit[\num{9.069454e9}]{\unit[\mathbb{M}]{\si{\per\kilogram}}}}{\left(\unit[10^{6}]{\mathbb{L}/}\unit[48]{\si{\metre}}\right)\left(\unit[1]{\unit[\mathbb{T}]{\si{\per\second}}}\right)}\right)\\
 & =\unit[\num{1.0966e4}]{\mathbb{M}\mathbb{L}^{-1}\mathbb{T}^{n-2}}
\end{align*}

\end_inset


